<script type="text/javascript">
	function exit(){
		$.ajax({  
	        type: "POST",  
	        async: false,  
	        url: '/theTest2/tUser/exit', 
	        contentType:"application/json",
	        dataType : "json",
	        success: function (result) { 
	           	if(result == 1){
	           		var url = '<%=basePath%>welcome.jsp';
	           		window.location.href = url ;
	            }
	        }
	    });
	}
	
	
</script>
<body>
